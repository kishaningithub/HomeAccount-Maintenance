 <script>
 </script>
 <script>
 /*
  *  Initialises elements inside daily expenses
  */
 function dailyExpenseData()
 {
   $("#date").datepicker({dateFormat:'dd-M-yy'});
   $("#incomeTypes").load("scripts/selects.html #incomeTypes option");
   $("#expenseTypes").load("scripts/selects.html #expenseTypes option");
 }
 /* Does the Initial Configuration of the screen
  * 1)Loads the mainmenu
  * 2)Loads individual screens inside the main menu
  * 3)Associates DatePicker with Date
  * 4)Loads the by income and expense types by retrieving values from the server 
  */
  function main()
  {
   $("#dailyExpense").load("scripts/DailyExpense.html",dailyExpenseData);
   $("#MainMenu").tabs();
  }
  $(document).ready(main);
 </script>
